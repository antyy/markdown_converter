{
  "version": "1.0.0",
  "sourceFormat": "md",
  "targetFormat": "html",
  "mappings": [
    {
      "src": "\\[(.+?)\\]\\((.+?)\\)",
      "dest": "<a href=\"%2$s\">%1$s</a>",
      "level": 1
    },
    {
      "src": "\\*\\*(.+?)\\*\\*",
      "dest": "<b>%s</b>",
      "level": 1
    },
    {
      "src": "^#\\s(.+)",
      "dest": "<h1>%s</h1>",
      "level": 0
    },
    {
      "src": "^##\\s(.+)",
      "dest": "<h2>%s</h2>",
      "level": 0
    },
    {
      "src": "^###\\s(.+)",
      "dest": "<h3>%s</h3>",
      "level": 0
    },
    {
      "src": "^####\\s(.+)",
      "dest": "<h4>%s</h4>",
      "level": 0
    },
    {
      "src": "^#####\\s(.+)",
      "dest": "<h5>%s</h5>",
      "level": 0
    },
    {
      "src": "^######\\s(.+)",
      "dest": "<h6>%s</h6>",
      "level": 0
    }
  ]
}