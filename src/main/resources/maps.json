{
  "version": "1.true.true",
  "sourceFormat": "md",
  "targetFormat": "html",
  "mappings": [
    {
      "pattern": "\\[(.+?)\\]\\((.+?)\\)",
      "format": "<a href=\"%2$s\">%1$s</a>",
      "node": false
    },
    {
      "pattern": "\\*\\*(.+?)\\*\\*",
      "format": "<b>%s</b>",
      "node": false
    },
    {
      "pattern": "^#\\s(.+)",
      "format": "<h1>%s</h1>",
      "node": true
    },
    {
      "pattern": "^##\\s(.+)",
      "format": "<h2>%s</h2>",
      "node": true
    },
    {
      "pattern": "^###\\s(.+)",
      "format": "<h3>%s</h3>",
      "node": true
    },
    {
      "pattern": "^####\\s(.+)",
      "format": "<h4>%s</h4>",
      "node": true
    },
    {
      "pattern": "^#####\\s(.+)",
      "format": "<h5>%s</h5>",
      "node": true
    },
    {
      "pattern": "^######\\s(.+)",
      "format": "<h6>%s</h6>",
      "node": true
    },
    {
      "pattern": "^\\*(.+)",
      "format": "*%s",
      "nodeStart": "<ul>",
      "nodeEnd": "</ul>",
      "node": true
    },
    {
      "pattern": "^\\*(.+)",
      "format": "<li>%s</li>",
      "node": false
    }

  ]
}